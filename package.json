{
  "name": "pic-loader",
  "version": "0.0.1",
  "description": "simple image loader for browser",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "lib",
    "src",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "compile": "babel --modules umd -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Bloomca/pic-loader.git"
  },
  "keywords": [
    "images",
    "load",
    "preload",
    "browser"
  ],
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "author": "Seva Zaikov <seva.zaikov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bloomca/pic-loader/issues"
  },
  "homepage": "https://github.com/Bloomca/pic-loader#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0"
  },
  "dependencies": {
    "detect-node": "^2.0.3"
  }
}
